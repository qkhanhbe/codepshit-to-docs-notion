# ===== Notion credentials =====
NOTION_API_KEY=your_notion_api_key_here
NOTION_DATABASE_ID=your_notion_database_id_here

# ===== Target submissions list URL (your logged-in page) =====
LIST_URL=https://code.ptit.edu.vn/student/history

# ===== Auth via browser cookies =====
# Copy the cookie string from DevTools -> Network -> any request to the site -> Request Headers -> Cookie
# Paste the whole string here (no quotes).
# sessionid=...; otherkey=value
COOKIE_STRING=your_cookie_string_here
# Optional: custom user agent to look like a normal browser
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120 Safari/537.36

# ===== Scraper selectors =====
# If the table is a standard <table>, these defaults often work.
ROW_SELECTOR=table tr
COL_INDEXES=0,1,2,3,4,5,6

# (Advanced) If you prefer CSS selectors relative to each row, set them and ignore COL_INDEXES.
#ID_CELL_SELECTOR=td:nth-child(1)
#TIME_CELL_SELECTOR=td:nth-child(2)
#PROBLEM_CELL_SELECTOR=td:nth-child(3)
#RESULT_CELL_SELECTOR=td:nth-child(4)
#PROBLEM_LINK_SELECTOR=a

# ===== Pagination (optional) =====
# ENABLE_PAGINATION=false
# PAGE_PARAM=page
# MAX_PAGES=3

# ===== Notion write rate limiting (seconds) =====
NOTION_RATE_DELAY=0.5
